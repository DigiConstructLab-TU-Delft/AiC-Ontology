        <rdfs:label xml:lang="en">contains activity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aic#containsTask -->


    <owl:ObjectProperty rdf:about="https://w3id.org/aic#containsTask">
