        <rdfs:range rdf:resource="https://w3id.org/aic#Process"/>
        <rdfs:comment xml:lang="en">Links a process to its preceding process as defined in the work breakdown structure.</rdfs:comment>
        <rdfs:label xml:lang="en">has predecessor</rdfs:label>
    </owl:ObjectProperty>
    


