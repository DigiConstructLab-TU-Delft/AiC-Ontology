        <rdfs:range rdf:resource="https://w3id.org/aic#AgentCapability"/>
        <rdfs:comment xml:lang="en">Indicates a capability that a construction agent is able to perform.</rdfs:comment>
        <rdfs:label xml:lang="en">has capability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aic#hasElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/aic#hasElement">
