        <rdfs:label xml:lang="en">corresponds to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aic#focusesOnElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/aic#focusesOnElement">
