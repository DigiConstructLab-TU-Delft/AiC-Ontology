        <rdfs:range rdf:resource="https://w3id.org/aic#Element"/>
        <rdfs:comment xml:lang="en">Indicates that an element is situated within or associated with a zone. A zone may represent a physical area or a conceptual operational area.</rdfs:comment>
        <rdfs:label xml:lang="en">has element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aic#hasInformationDimension -->


