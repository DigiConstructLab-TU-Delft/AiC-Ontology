        <rdfs:range rdf:resource="https://w3id.org/aic#ProcessMetric"/>
        <rdfs:comment xml:lang="en">Connects a process with a metric that describes its performance.</rdfs:comment>
        <rdfs:label xml:lang="en">has process metric</rdfs:label>
    </owl:ObjectProperty>
    


